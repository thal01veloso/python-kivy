<Aplicativo@FloatLayout>:
	AsyncImage:
		source: 'fundo.png'
		size_hint_x: 100
		size_hint_y: 100
		pos_hint: {'center_x':.5, 'center_y': .5}

	Button:
		size_hint_x: .3
		size_hint_y: .1
		text: '+1'
		pos_hint:{"x":.10,"y":.2}
		font_size: '50sp'
		on_press: root.soma()
				
	Button:
		size_hint_x: .3
		size_hint_y: .1
		text:'-1'
		font_size: '50sp'
		pos_hint: {"x":.60,"y":.2}
		on_press: root.subt()
	Button:
		size_hint_x: .3
		size_hint_y: .1
		text:'Reset'
		font_size: '30sp'
		pos_hint: {"x":.35,"y":.01}
		on_press: root.reset()
	Label:
		size_hint_x: .3
		size_hint_y: .1
		text:'0'
		font_size: '150sp'
		pos_hint: {"center_x":.50,"center_y":.6}
		id: lb
		outline_color: 5, 5, 5, 1
